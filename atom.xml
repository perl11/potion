---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>whymirror.github.com/potion</title>
 <link href="http://whymirror.github.com/potion/atom.xml" rel="self"/>
 <link href="http://whymirror.github.com/potion/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://whymirror.github.com/</id>
 <author>
   <name>whymirror.github.com</name>
   <email>message-whymirror@on.github.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://whymirror.github.com/potion/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://whymirror.github.com/potion/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
